{
  "name": "bass-notes",
  "version": "0.0.1",
  "description": "Save selected texts into note which can be accessed later and opened as a link.",
  "keywords": [
    "chrome-extension",
    "notes"
  ],
  "author": "Hanna",
  "license": "MIT",
  "homepage": "https://github.com/JoySR/bass-notes",
  "repository": "github:JoySR/bass-notes",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/webpack/bin/webpack.js --watch --config config/webpack.config.js",
    "build": "export NODE_ENV=production && npm run build_pro",
    "build_pro": "./node_modules/webpack/bin/webpack.js --config config/webpack.pro.js -p",
    "build_dev": "./node_modules/webpack/bin/webpack.js --config config/webpack.config.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.5.1",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-core": "^6.14.0",
    "babel-preset-react-app": "^3.1.1",
    "clean-webpack-plugin": "^0.1.10",
    "es6-promise": "^3.2.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "webpack": "^1.13.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "keyboardjs": "^2.3.1"
  }
}
